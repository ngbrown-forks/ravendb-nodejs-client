{
  "name": "ravendb",
  "version": "6.0.0",
  "description": "RavenDB client for Node.js",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "homepage": "https://ravendb.net",
  "main": "./dist/cjs/ravendb.cjs",
  "types": "dist/ravendb.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/ravendb.d.ts",
      "import": "./dist/ravendb.js",
      "require": "./dist/cjs/ravendb.cjs"
    }
  },
  "scripts": {
    "test": "mocha \"test/**/*.ts\"",
    "build": "tsup",
    "watch": "tsc --watch",
    "lint": "eslint {src,test}/**/*.ts",
    "check-exports": "node ./scripts/reportMissingTopLevelExports.js",
    "check-imports": "node scripts/clearGlobalExports.js && tsc -p tsconfig.imports-check.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ravendb/ravendb-nodejs-client"
  },
  "bugs": {
    "email": "support@ravendb.net",
    "url": "http://issues.hibernatingrhinos.com"
  },
  "contributors": [
    "Hibernating Rhinos"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ravendb",
    "ravendb-client",
    "typescript",
    "node",
    "nodejs",
    "nosql"
  ],
  "author": "Hibernating Rhinos",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/mocha": "^10.0.3",
    "@types/rimraf": "^4.0.5",
    "@types/sinon": "^17.0.3",
    "@types/unzipper": "^0.10.5",
    "cross-os": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "glob": "^10.3.12",
    "globals": "^15.0.0",
    "http-proxy-agent": "^7.0.2",
    "lodash.orderby": "^4.6.0",
    "mocha": "^10.2.0",
    "open": "^10.1.0",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.5.0",
    "unzipper": "^0.10.11"
  },
  "dependencies": {
    "@types/node": "^20.12.4",
    "@types/node-fetch": "^2.6.2",
    "@types/pluralize": "^0.0.33",
    "@types/stream-json": "^1.7.2",
    "@types/ws": "^8.5.10",
    "change-case": "^5.4.4",
    "form-data": "^4.0.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "pluralize": "^8.0.0",
    "safe-memory-cache": "^2.0.0",
    "stream-json": "^1.8.0",
    "ws": "^8.16.0"
  }
}
